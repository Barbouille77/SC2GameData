<?xml version="1.0" encoding="us-ascii"?>
<Catalog>
    <CSkin id="SuperSonyaPink" parent="SuperSonyaCommon">
        <AttributeId value="SuS2"/>
        <ModelMacroRun Models="SuperSonyaModels" Macro="SuperSonyaPinkMacro"/>
        <ModelMacroRun Models="SuperSonyaEffectModels" Macro="SuperSonyaPinkEffectMacro"/>
        <VariationIcon value="Assets\Textures\UI_Glues_Swatch_Purple.dds"/>
    </CSkin>
    <CTexture id="Storm_D3Barbarian_Super_Pink_Diff" parent="MainSlotTexture"/>
    <CTexture id="Storm_D3Barbarian_Super_Pink_Emis" parent="MainSlotTexture"/>
    <CTexture id="Storm_D3BarbarianEye_Super_Pink_Deca" parent="Generic01SlotTexture"/>
    <CTexture id="Storm_D3BarbarianEyeGleam_Super_Pink_Diff" parent="Generic01SlotTexture"/>
    <CTexture id="Storm_Barbarian_Super_Spear_Pink_Diff" parent="MainSlotTexture"/>
    <CActorEventMacroRunnable id="SuperSonyaPinkMacro">
        <Do Send="TextureSelectById Storm_D3Barbarian_Super_Pink_Diff"/>
        <Do Send="TextureSelectById Storm_D3Barbarian_Super_Pink_Emis"/>
        <Do Send="TextureSelectById Storm_D3Barbarian_Super_Blue_Spec"/>
        <!-- not a typo, this should use blue per task 53996 -->
        <Do Send="TextureSelectById Storm_D3BarbarianEye_Super_Pink_Deca"/>
        <Do Send="TextureSelectById Storm_D3BarbarianEyeGleam_Super_Pink_Diff"/>
    </CActorEventMacroRunnable>
    <CActorEventMacroRunnable id="SuperSonyaPinkEffectMacro">
        <Do Send="TextureSelectById Storm_Barbarian_Super_Spear_Pink_Diff"/>
    </CActorEventMacroRunnable>
</Catalog>
