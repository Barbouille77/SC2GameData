<?xml version="1.0" encoding="us-ascii"?>
<tree>
    <include file="GameData/AI/Minion.aitree"/>
    
    <!-- Move attack structures above attack minions. -->
    <mod id="LeashSelect">
        <moveChild id="LeashAttackStructures" before="LeashAttackMinionsToLane"/>
    </mod>
    <mod id="Root">
        <moveChild id="AttackStructures" before="AttackMinions"/>
    </mod>
    
    <!-- We no longer care about ignoring targets behind gates because we'll attack gates first anyway. -->
    <mod id="AttackMinions">
        <property name="IgnoreBehindGate" value="false"/>
    </mod>
</tree>
