<?xml version="1.0" encoding="us-ascii"?>
<Catalog>
    <CActorModel id="StukovMassiveShoveFirstSegment">
        <HostSiteOps Ops="SOp2DRotation SOpOffsetLaunch"/>
    </CActorModel>
    <CActorModel id="FalstadHinterlandBlastWarningVisibilityShapeModel">
        <Remove Terms="ActorCreation" Send="Create HinterlandBlastWarningBeamAttackBeam"/>
        <On Terms="ActorCreation" Send="TimerSet 0.062500 Warning"/>
        <On Terms="TimerExpired; TimerName Warning; ValidatePlayer HasFalstadCalloftheWildhammer" Send="Create HinterlandBlastWarningBeamAttackBeamUpgraded"/>
        <On Terms="TimerExpired; TimerName Warning; !ValidatePlayer HasFalstadCalloftheWildhammer" Send="Create HinterlandBlastWarningBeamAttackBeam"/>
        <On Terms="ActorDestruction" Target="HinterlandBlastWarningBeamAttackBeamUpgraded" Send="Destroy"/>
    </CActorModel>
    <CActorModel id="FalstadHinterlandBlastVisibilityShapeModel">
        <Remove Terms="ActorCreation" Send="Create HinterlandBlastBeamAttackBeam"/>
        <On Terms="ActorCreation; ValidatePlayer HasFalstadCalloftheWildhammer" Send="Signal HinterlandBlastBeamAttackBeamUpgraded"/>
        <On Terms="ActorCreation; !ValidatePlayer HasFalstadCalloftheWildhammer" Send="Signal HinterlandBlastBeamAttackBeam"/>
        <On Terms="Signal.*.HinterlandBlastBeamAttackBeamUpgraded" Send="Create HinterlandBlastBeamAttackBeamUpgraded"/>
        <On Terms="Signal.*.HinterlandBlastBeamAttackBeam" Send="Create HinterlandBlastBeamAttackBeam"/>
    </CActorModel>
    <CActorBeamSimple id="HinterlandBlastWarningBeamAttackBeam">
        <HostImpactSiteOps Ops="SOpAttachOrigin SOpHinterland_Base SOpShadow SOpMoveUpBy2"/>
        <On Terms="Abil.FalstadHinterlandBlast.SourceCastStop" Send="HostSiteOpsSet ::HostImpact {SOpAttachOrigin SOpHinterland_Base SOpShadow SOpMoveUpBy2} 1 1"/>
    </CActorBeamSimple>
    <CActorBeamSimple id="HinterlandBlastWarningBeamAttackBeamUpgraded" parent="HinterlandBlastWarningBeamAttackBeam">
        <HostImpactSiteOps Ops="SOpAttachOrigin SOpHinterland_Upgraded SOpShadow SOpMoveUpBy2"/>
        <On Terms="Abil.FalstadHinterlandBlast.SourceCastStop" Send="HostSiteOpsSet ::HostImpact {SOpAttachOrigin SOpHinterland_Upgraded SOpShadow SOpMoveUpBy2} 1 1"/>
    </CActorBeamSimple>
    <CActorSiteOpLocalOffset id="SOpHinterland_Upgraded">
        <LocalOffset value="0.000000,-61.500000,0.000000" Y="-61.500000"/>
    </CActorSiteOpLocalOffset>
    <CActorSiteOpLocalOffset id="SOpHinterland_Base">
        <LocalOffset value="0.000000,-30.000000,0.000000" Y="-30.000000"/>
    </CActorSiteOpLocalOffset>
    <CActorTriangle id="AlarakDiscordStrikeGroundWarning">
        <HostSiteOps Ops="SOpForwardTargetPoint SOpMoveForwardByDot5 SOpMoveForwardByDot15 SOpMoveForwardByDot1" HoldRotation="1"/>
    </CActorTriangle>
    <CActorTriangle id="AlarakDiscordStrikeTriangleLeftWarning">
        <HostSiteOps Ops="SOpForwardTargetPoint SOp90 AlarakDiscordStrikeLeftWarningOffset AlarakDiscordStrikeLeftInvert" HoldRotation="1"/>
    </CActorTriangle>
    <CActorTriangle id="AlarakDiscordStrikeTriangleRightWarning">
        <HostSiteOps Ops="SOpForwardTargetPoint SOp270 AlarakDiscordStrikeRightWarningOffset" HoldRotation="1"/>
    </CActorTriangle>
    <CActorModel id="AurielSacredSweepTrailFX">
        <On Terms="Effect.AurielSacredSweepInitialPersistent.Stop" Send="Destroy"/>
        <On Terms="Abil.AurielSacredSweep.SourceChannelStop" Send="HostSiteOpsSet ::Host {SOpSourcePoint SOpForwardCasterPointInverted} 1 1"/>
    </CActorModel>
    <CActorModel id="AurielSacredSweepGroundFX">
        <On Terms="Abil.AurielSacredSweep.SourceChannelStop" Send="HostSiteOpsSet ::Host {SOpSourcePoint SOpForwardCasterPointInverted} 1 1"/>
        <HostSiteOps Ops="SOpSourcePoint SOpForwardCasterPointInverted" HoldRotation="1"/>
    </CActorModel>
    <CActorArc id="GarroshGroundbreakerAllySecondWarningArcGuide">
        <HostSiteOps Ops="SOpSourcePoint SOpForwardTargetPoint" HoldRotation="1"/>
    </CActorArc>
    <CActorArc id="GarroshGroundbreakerAllyWarningArcGuide">
        <HostSiteOps Ops="SOpSourcePoint SOpForwardTargetPoint" HoldRotation="1"/>
    </CActorArc>
    <CActorModel id="Storm_Effect_BrightWing_Base_PhaseShift_Cast">
        <On Terms="Effect.BrightwingPhaseShiftFlightTeleport.Start" Send="HostSiteOpsSet ::Host SOpAttachOrigin 1 1"/>
        <HostSiteOps Ops="SOpAttachOrigin"/>
    </CActorModel>
</Catalog>
