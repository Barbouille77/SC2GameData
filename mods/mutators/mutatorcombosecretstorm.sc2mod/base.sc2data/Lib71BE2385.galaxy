include "TriggerLibs/NativeLib"
include "LibCOMU"

include "Lib71BE2385_h"

//--------------------------------------------------------------------------------------------------
// Library: WeeklyChallenge
//--------------------------------------------------------------------------------------------------
// External Library Initialization
void lib71BE2385_InitLibraries () {
    libNtve_InitVariables();
    libCOMU_InitVariables();
}

// Triggers
//--------------------------------------------------------------------------------------------------
// Trigger: Initialization
//--------------------------------------------------------------------------------------------------
bool lib71BE2385_gt_Initialization_Func (bool testConds, bool runActions) {
    // Automatic Variable Declarations
    // Actions
    if (!runActions) {
        return true;
    }

    libCOMU_gf_SetMutatorWeeklyChallengeOn(3223);
    return true;
}

//--------------------------------------------------------------------------------------------------
void lib71BE2385_gt_Initialization_Init () {
    lib71BE2385_gt_Initialization = TriggerCreate("lib71BE2385_gt_Initialization_Func");
    TriggerAddEventMapInit(lib71BE2385_gt_Initialization);
}

void lib71BE2385_InitTriggers () {
    lib71BE2385_gt_Initialization_Init();
}

//--------------------------------------------------------------------------------------------------
// Library Initialization
//--------------------------------------------------------------------------------------------------
bool lib71BE2385_InitLib_completed = false;

void lib71BE2385_InitLib () {
    if (lib71BE2385_InitLib_completed) {
        return;
    }

    lib71BE2385_InitLib_completed = true;

    lib71BE2385_InitLibraries();
    lib71BE2385_InitTriggers();
}

