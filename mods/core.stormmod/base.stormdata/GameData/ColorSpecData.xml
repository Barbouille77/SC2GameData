<Catalog>
    <CColorSpec default="1">
        <!-- VALID SPECIFICATIONS METHODS
            value="R,G,B"                       Default HDR-Multiplier values are all 1s
            value="R,G,B, hdrAll"               All HDR-Multiplier values are set to hdrAll
            value="R,G,B, hdrR,hdrG,hdrB"       Specifying everything.  Notice,no alpha value.
            
            R, G, and B are in the range: [0, 255].  The HDR-Multipliers are: [0.0, ...)
            Notice there NO SPACES between thr R G and B
        -->
        <Color index="Diffuse" value="0,0,0, 1.000"/>
        <Color index="Emissive" value="0,0,0, 1.000"/>
    </CColorSpec>

