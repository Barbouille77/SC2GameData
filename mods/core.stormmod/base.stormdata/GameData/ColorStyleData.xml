            <Value index="Colorblind" value="ColorHOTOwner"/>
        </ColorEntry>
        <ColorEntry index="Ally">
            <Value index="Default" value="ColorHOTAlly"/>
            <Value index="Colorblind" value="ColorHOTAllyColorblind"/>
        </ColorEntry>
        <ColorEntry index="Enemy">
            <Value index="Default" value="ColorHOTEnemy"/>
            <Value index="Colorblind" value="ColorHOTEnemyColorblind"/>
        </ColorEntry>
        <ColorEntry index="TeamOrder">
            <Value index="Default" value="ColorHOTAlly"/>
            <Value index="Colorblind" value="ColorHOTAllyColorblind"/>
        </ColorEntry>
        <ColorEntry index="TeamChaos">
            <Value index="Default" value="ColorHOTEnemy"/>
            <Value index="Colorblind" value="ColorHOTEnemyColorblind"/>
