    <CColorStyle id="OpposingTeam">
        <ColorEntry index="DefaultNeutral">
            <Value index="Default" value="ColorAllianceTeamHostile"/>
            <Value index="Colorblind" value="ColorAllianceTeamHostileColorblind"/>
        </ColorEntry>
        <ColorEntry index="Owner">
            <Value index="Default" value="ColorAllianceEnemy"/>
            <Value index="Colorblind" value="ColorAllianceEnemyColorblind"/>
        </ColorEntry>
        <ColorEntry index="Ally">
            <Value index="Default" value="ColorAllianceEnemy"/>
            <Value index="Colorblind" value="ColorAllianceEnemyColorblind"/>
        </ColorEntry>
        <ColorEntry index="Enemy">
            <Value index="Default" value="ColorAllianceAlly"/>
            <Value index="Colorblind" value="ColorAllianceAllyColorblind"/>
        </ColorEntry>
        <ColorEntry index="TeamHostile">
            <Value index="Default" value="ColorAllianceNeutral"/>
            <!-- For some reason this colorblind color is not White like the Primary-Style's Neutral-Colorblind -->
            <Value index="Colorblind" value="ColorAllianceNeutralColorblind"/>
        </ColorEntry>
        <ColorEntry index="TeamOrder">
            <Value index="Default" value="ColorAllianceEnemy"/>
            <Value index="Colorblind" value="ColorAllianceEnemyColorblind"/>
