        <ColorEntry index="TeamOrder">
            <Value index="Default" value="ColorMapMechanicAlly"/>
            <Value index="Colorblind" value="ColorMapMechanicAlly"/>
        </ColorEntry>
        <ColorEntry index="Ally">
            <Value index="Default" value="ColorMapMechanicAlly"/>
            <Value index="Colorblind" value="ColorMapMechanicAlly"/>
        </ColorEntry>
        <ColorEntry index="TeamChaos">
            <Value index="Default" value="ColorMapMechanicEnemy"/>
            <Value index="Colorblind" value="ColorMapMechanicEnemyColorblind"/>
        </ColorEntry>
        <ColorEntry index="Enemy">
            <Value index="Default" value="ColorMapMechanicEnemy"/>
            <Value index="Colorblind" value="ColorMapMechanicEnemyColorblind"/>
        </ColorEntry>
    </CColorStyle>
</Catalog>