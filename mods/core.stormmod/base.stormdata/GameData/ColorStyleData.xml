<?xml version="1.0" encoding="us-ascii"?>
<Catalog>
    <CColorStyle default="1">
        <Name value="ColorStyle/Name/##id##"/>
    </CColorStyle>

    <CColorStyle id="AllWhite">
        <ColorEntry index="DefaultNeutral">
            <Value index="Default" value="ColorWhite"/>
        </ColorEntry>
    </CColorStyle>

    <CColorStyle id="AllGray">
        <ColorEntry index="DefaultNeutral">
            <Value index="Default" value="ColorLeoricGray"/>
        </ColorEntry>
    </CColorStyle>

    <CColorStyle id="Primary">
        <ColorEntry index="DefaultNeutral">
            <Value index="Default" value="ColorAllianceNeutral"/>
            <Value index="Colorblind" value="ColorWhite"/>
        </ColorEntry>
        <ColorEntry index="Owner">
            <Value index="Default" value="ColorAllianceSelf"/>
            <Value index="Colorblind" value="ColorAllianceSelfColorblind"/>
        </ColorEntry>
        <ColorEntry index="Ally">
            <Value index="Default" value="ColorAllianceAlly"/>
            <Value index="Colorblind" value="ColorAllianceAllyColorblind"/>
        </ColorEntry>
        <ColorEntry index="Enemy">
            <Value index="Default" value="ColorAllianceEnemy"/>
            <Value index="Colorblind" value="ColorAllianceEnemyColorblind"/>
        </ColorEntry>
        <ColorEntry index="TeamHostile">
            <Value index="Default" value="ColorAllianceTeamHostile"/>
            <Value index="Colorblind" value="ColorAllianceTeamHostileColorblind"/>
        </ColorEntry>
        <ColorEntry index="TeamOrder">
            <Value index="Default" value="ColorAllianceAlly"/>
            <Value index="Colorblind" value="ColorAllianceAllyColorblind"/>
        </ColorEntry>
        <ColorEntry index="TeamChaos">
            <Value index="Default" value="ColorAllianceEnemy"/>
            <Value index="Colorblind" value="ColorAllianceEnemyColorblind"/>
        </ColorEntry>
    </CColorStyle>

    <CColorStyle id="MinimapTeams" parent="Primary">
        <!-- Same as Primary except you have the same blue color as your teammates -->
        <ColorEntry index="Owner">
            <Value index="Default" value="ColorAllianceAlly"/>
            <Value index="Colorblind" value="ColorAllianceAllyColorblind"/>
        </ColorEntry>
    </CColorStyle>

    <CColorStyle id="PrimaryLight">
        <ColorEntry index="DefaultNeutral">
            <Value index="Default" value="ColorAllianceNeutralLight"/>
            <Value index="Colorblind" value="ColorAllianceNeutralLightColorblind"/>
        </ColorEntry>
        <ColorEntry index="Owner">
            <Value index="Default" value="ColorAllianceSelfLight"/>
            <Value index="Colorblind" value="ColorAllianceSelfLightColorblind"/>
        </ColorEntry>
        <ColorEntry index="Ally">
            <Value index="Default" value="ColorAllianceAllyLight"/>
            <Value index="Colorblind" value="ColorAllianceAllyLightColorblind"/>
        </ColorEntry>
        <ColorEntry index="Enemy">
            <Value index="Default" value="ColorAllianceEnemyLight"/>
            <Value index="Colorblind" value="ColorAllianceEnemyLightColorblind"/>
        </ColorEntry>
        <ColorEntry index="TeamHostile">
            <Value index="Default" value="ColorAllianceTeamHostileLight"/>
            <Value index="Colorblind" value="ColorAllianceTeamHostileLightColorblind"/>
        </ColorEntry>
        <ColorEntry index="TeamOrder">
            <Value index="Default" value="ColorAllianceAllyLight"/>
            <Value index="Colorblind" value="ColorAllianceAllyLightColorblind"/>
        </ColorEntry>
        <ColorEntry index="TeamChaos">
            <Value index="Default" value="ColorAllianceEnemyLight"/>
            <Value index="Colorblind" value="ColorAllianceEnemyLightColorblind"/>
        </ColorEntry>
    </CColorStyle>
    
    <CColorStyle id="MinimapTeamsLight" parent="PrimaryLight">
        <!-- Same as PrimaryLight except you have the same blue color as your teammates -->
        <ColorEntry index="Owner">
            <Value index="Default" value="ColorAllianceAllyLight"/>
            <Value index="Colorblind" value="ColorAllianceAllyLightColorblind"/>
        </ColorEntry>
    </CColorStyle>

    <CColorStyle id="Shields" parent="AllWhite">
    </CColorStyle>

    <CColorStyle id="Energy" parent="AllWhite">
    </CColorStyle>
    
    <CColorStyle id="Reduction">
        <ColorEntry index="DefaultNeutral">
            <Value index="Default" value="ColorHealthbarReduction"/>
        </ColorEntry>
    </CColorStyle>

    <CColorStyle id="HoT">
