//--------------------------------------------------------------------------------------------------
bool libHSAM_gt_SamuroSelectionChange_Func (bool testConds, bool runActions) {
    unitgroup auto078DF349_g;
    int auto078DF349_u;

    // Variable Declarations
    unit lv_ClosestUnit;
    unit lv_itUnit;

    // Variable Initialization

    // Conditions
    if (testConds) {
        if (!(((UnitGetType(libGame_gv_players[EventPlayer()].lv_heroUnit) == "HeroSamuro") || (UnitGetType(libGame_gv_players[EventPlayer()].lv_activeVehicle) == "HeroSamuro")))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    auto078DF349_g = UnitGroupSelected(EventPlayer());
    auto078DF349_u = 1;
    for ( ; auto078DF349_u <= UnitGroupCount(auto078DF349_g, c_unitCountAll) ; auto078DF349_u += (lv_itUnit == UnitGroupUnit(auto078DF349_g, auto078DF349_u)) ) {
        lv_itUnit = UnitGroupUnit(auto078DF349_g, auto078DF349_u);
        if ((UnitGetType(lv_itUnit) == "HeroSamuro")) {
            libGame_gv_players[EventPlayer()].lv_cameraPlayerUnitGroupToFocusOn = libNtve_gf_ConvertUnitToUnitGroup(lv_itUnit);
            libGame_gf_CameraUpdateCameraForPlayer(EventPlayer());
            return true;
        }

    }
    lv_ClosestUnit = UnitGroupClosestToPoint(UnitGroupSelected(EventPlayer()), CameraGetTarget(EventPlayer()));
    if ((UnitGetType(lv_ClosestUnit) == "SamuroMirrorImage")) {
        libGame_gv_players[EventPlayer()].lv_cameraPlayerUnitGroupToFocusOn = libNtve_gf_ConvertUnitToUnitGroup(lv_ClosestUnit);
        libGame_gf_CameraUpdateCameraForPlayer(EventPlayer());
    }

    return true;
}

