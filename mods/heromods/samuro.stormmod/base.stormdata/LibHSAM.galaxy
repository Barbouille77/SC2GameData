//--------------------------------------------------------------------------------------------------
// Trigger: Samuro - Use Vehicle
//--------------------------------------------------------------------------------------------------
bool libHSAM_gt_SamuroUseVehicle_Func (bool testConds, bool runActions) {
    // Variable Declarations
    int lv_controlGroup;
    int lv_samuroPlayer;
    unit lv_vehicle;
    unit lv_samuro;

    // Variable Initialization

    // Conditions
    if (testConds) {
        if (!((UnitGetType(libGame_gv_players[UnitGetOwner(EventPlayerEffectUsedUnit(c_effectUnitCaster))].lv_heroUnit) == "HeroSamuro"))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    lv_vehicle = EventPlayerEffectUsedUnit(c_effectUnitTarget);
    lv_samuro = EventPlayerEffectUsedUnit(c_effectUnitCaster);
    lv_samuroPlayer = UnitGetOwner(lv_samuro);
    Wait(0.0625, c_timeGame);
    UnitSelect(lv_vehicle, lv_samuroPlayer, true);
    if ((PlayerHasTalent(lv_samuroPlayer, "SamuroHeroicAbilityIllusionMaster") == true)) {
        UnitControlGroupAddUnit(lv_samuroPlayer, 1, lv_vehicle);
        UnitControlGroupRemoveUnit(lv_samuroPlayer, 1, lv_samuro);
        UnitControlGroupRemoveUnit(lv_samuroPlayer, 4, lv_samuro);
        UnitControlGroupAddUnit(lv_samuroPlayer, 4, lv_vehicle);
        UnitInventoryCreate(lv_vehicle, "SamuroIllusionMasterToggleAIItem");
    }

    while ((UnitIsAlive(lv_vehicle) == true)) {
        UnitSetPosition(lv_samuro, UnitGetPosition(lv_vehicle), true);
        if ((UnitIsSelected(lv_vehicle, lv_samuroPlayer) == true)) {
            libGame_gv_players[lv_samuroPlayer].lv_cameraPlayerUnitGroupToFocusOn = libNtve_gf_ConvertUnitToUnitGroup(lv_vehicle);
            libGame_gf_CameraUpdateCameraForPlayer(lv_samuroPlayer);
        }

        Wait(0.0625, c_timeGame);
    }
    if ((PlayerHasTalent(lv_samuroPlayer, "SamuroHeroicAbilityIllusionMaster") == true)) {
        UnitControlGroupRemoveUnit(lv_samuroPlayer, 1, lv_vehicle);
        UnitControlGroupAddUnit(lv_samuroPlayer, 1, lv_samuro);
        UnitControlGroupAddUnit(lv_samuroPlayer, 4, lv_samuro);
        UnitControlGroupRemoveUnit(lv_samuroPlayer, 4, lv_vehicle);
    }

    UnitSelect(lv_samuro, lv_samuroPlayer, true);
    return true;
}

