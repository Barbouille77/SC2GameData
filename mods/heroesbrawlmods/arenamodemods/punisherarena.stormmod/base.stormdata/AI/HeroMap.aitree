<?xml version="1.0" encoding="us-ascii"?>
<tree>
    <include file="AI/HeroBase.aitree"/>

    <!--We want to prioritize and fight heroes around the punisher-->
    <mod id="ATTACKTARGET_AttemptFight">
        <moveChild id="ATTACKTARGET_Fight" before="ATTACKTARGET_FightAttackTarget"/>
    </mod>

    <mod id="ATTACKTARGET_LowThreatAttemptFight">
        <moveChild id="ATTACKTARGET_LowThreatFight" before="ATTACKTARGET_LowThreatFightAttackTarget"/>
    </mod>
</tree>

